{
  "name": "api-node-login",
  "version": "2.3.0",
  "license": "MIT",
  "scripts": {
    "start": "node -r esm src/server.js",
    "test": "jasmine",
    "dev": "nodemon -r esm --inspect src/server.js",
    "lint": "eslint --ignore-pattern /build/** .",
    "build": "babel src --ignore src/**/*.spec.js --delete-dir-on-start --out-dir build",
    "coverage": "c8 --reporter=lcov --reporter=text --reporter=text-summary --all --include=src/**/*.js --exclude=src/**/*.spec.js jasmine"
  },
  "files": [
    "build",
    ".sequelizerc"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "c8": "^7.0.1",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jasmine": "^4.1.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "esm": "^3.2.25",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.18.2",
    "sequelize-cli": "^5.5.1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.1",
    "date-fns": "^2.9.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql2": "^1.7.0",
    "sequelize": "^5.18.1",
    "winston": "^3.2.1",
    "yup": "^0.28.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "optionalDependencies": {
    "aws-sdk": "^2.613.0"
  }
}
